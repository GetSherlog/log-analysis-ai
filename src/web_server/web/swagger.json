{
  "openapi": "3.0.0",
  "info": {
    "title": "LogAI-CPP Web API",
    "description": "High-performance web API exposing LogAI-CPP library functionality",
    "version": "0.1.0",
    "contact": {
      "name": "LogAI Team"
    }
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Local development server"
    }
  ],
  "paths": {
    "/health": {
      "get": {
        "summary": "Server health check",
        "description": "Returns the current status of the server",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    },
                    "version": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api": {
      "get": {
        "summary": "API information",
        "description": "Returns information about the API",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "version": {
                      "type": "string"
                    },
                    "status": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/parser/drain": {
      "post": {
        "summary": "Parse log lines using DRAIN algorithm",
        "description": "Parses log lines using the DRAIN algorithm to extract templates",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["logLines"],
                "properties": {
                  "logLines": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "depth": {
                    "type": "integer",
                    "default": 4
                  },
                  "similarityThreshold": {
                    "type": "number",
                    "default": 0.5
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "clusters": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "template": {
                            "type": "string"
                          },
                          "size": {
                            "type": "integer"
                          },
                          "logs": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            }
                          }
                        }
                      }
                    },
                    "totalClusters": {
                      "type": "integer"
                    },
                    "totalLogs": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/parser/file": {
      "post": {
        "summary": "Parse logs from a file",
        "description": "Parses logs from a file using the appropriate parser based on file type",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["filePath"],
                "properties": {
                  "filePath": {
                    "type": "string"
                  },
                  "maxLines": {
                    "type": "integer"
                  },
                  "delimiter": {
                    "type": "string"
                  },
                  "hasHeader": {
                    "type": "boolean"
                  },
                  "pattern": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "records": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "timestamp": {
                            "type": "string"
                          },
                          "message": {
                            "type": "string"
                          }
                        }
                      }
                    },
                    "totalRecords": {
                      "type": "integer"
                    },
                    "filePath": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "File Not Found"
          }
        }
      }
    },
    "/api/features/extract": {
      "post": {
        "summary": "Extract features from log lines",
        "description": "Extracts features from log lines for further analysis",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["logLines"],
                "properties": {
                  "logLines": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "features": {
                      "type": "array",
                      "items": {
                        "type": "array",
                        "items": {
                          "type": "number"
                        }
                      }
                    },
                    "totalFeatures": {
                      "type": "integer"
                    },
                    "featureDimension": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/api/features/logbert": {
      "post": {
        "summary": "Vectorize log messages using LogBERT",
        "description": "Converts log messages into vectors using LogBERT tokenization",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["logMessages"],
                "properties": {
                  "logMessages": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "maxSequenceLength": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "vectors": {
                      "type": "array",
                      "items": {
                        "type": "array",
                        "items": {
                          "type": "number"
                        }
                      }
                    },
                    "totalVectors": {
                      "type": "integer"
                    },
                    "vectorDimension": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/api/anomalies/ocsvm": {
      "post": {
        "summary": "Detect anomalies using One-Class SVM",
        "description": "Detects anomalies in log data using One-Class SVM algorithm",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["featureVectors"],
                "properties": {
                  "featureVectors": {
                    "type": "array",
                    "items": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  },
                  "kernel": {
                    "type": "string",
                    "enum": ["linear", "rbf", "poly"],
                    "default": "rbf"
                  },
                  "nu": {
                    "type": "number",
                    "default": 0.1
                  },
                  "gamma": {
                    "type": "number",
                    "default": 0.1
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "predictions": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    },
                    "anomalyIndices": {
                      "type": "array",
                      "items": {
                        "type": "integer"
                      }
                    },
                    "totalSamples": {
                      "type": "integer"
                    },
                    "anomalyCount": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/api/anomalies/dbscan": {
      "post": {
        "summary": "Cluster log events using DBSCAN",
        "description": "Clusters log events using DBSCAN algorithm",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["featureVectors"],
                "properties": {
                  "featureVectors": {
                    "type": "array",
                    "items": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  },
                  "eps": {
                    "type": "number",
                    "default": 0.5
                  },
                  "minSamples": {
                    "type": "integer",
                    "default": 5
                  },
                  "useKdTree": {
                    "type": "boolean",
                    "default": false
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "labels": {
                      "type": "array",
                      "items": {
                        "type": "integer"
                      }
                    },
                    "totalSamples": {
                      "type": "integer"
                    },
                    "numClusters": {
                      "type": "integer"
                    },
                    "outlierCount": {
                      "type": "integer"
                    },
                    "useKdTree": {
                      "type": "boolean"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  }
} 